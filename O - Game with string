void JustA_GININ_TryToSolve()
{
    string a;cin>>a;
    ll i,j,c=1;
    for(i=0;i<a.size();i++)
    {
        while ((a[i]==a[i+1]&&i+1<a.size())||(a[i]==a[i-1]&&i-1>=0))
        {
            a.erase(i,2);
            i--;
            c++;
        }
    }
    //cout<<a<<"<";el;
    if(c%2==0)//
        cout<<"YES\n";
    else
        cout<<"NO\n";
}
