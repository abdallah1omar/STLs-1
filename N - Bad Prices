void JustA_GININ_TryToSolve()
{
    ll n;cin>>n;
    vector<ll>ar(n);
    loop0(n)
    {
        cin>>ar[i];
    }
    ll minn = ar[n-1],c=0;
    for (ll i=n-2;i>=0;i--)
    {
        if(ar[i]>minn)
        {
            c++;
        }
        minn=min(ar[i],minn);
    }
    cout<<c;el;
}
