#define loop1(n) for(ll i=1;i<=n;i++)
#define loop0(n) for(ll i=0;i<n;i++)
#define pq priority_queue
using namespace std;
//ll pre[200001];
//ll ar[200001];
//ll freq[500002];
void JustA_GININ_TryToSolve()
{
    ll n;cin>>n;
    pq<ll> p;ll ans=0,tp;
    for (ll i=0,x;i<n;i++)
    {
        cin>>x;
        if(x!=0)
        {
            p.push(x);
        }else
        {
            if(!p.empty())
            {
                tp = p.top();
                ans+=tp;
                p.pop();
            }
        }
    }
    cout<<ans;el;
}
int main()
{
    NARUTO_3MK
    ll TC=1;
    cin>>TC;
    test{
        JustA_GININ_TryToSolve();
    }
    O2AF_3NDK_YA_MAGDY
}
